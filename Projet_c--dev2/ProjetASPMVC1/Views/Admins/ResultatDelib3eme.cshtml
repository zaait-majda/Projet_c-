@model IEnumerable<ProjetASPMVC1.Models.Candidat>

@{

    Layout = "~/Views/Shared/_LayoutAdmin.cshtml ";
}

<center>
    <h4>Résultat Délibération</h4>

</center>
@if (ViewBag.error != "")
{
    <center>
        <div class="alert alert-danger" role="alert">
           Vous n'avez aucun candidat préselectioné <a href="#" class="alert-link">Cliquer ici</a>. pour revenir au page de preselection.
        </div>
    </center>
}
else
{
    <center>
        <div class="alert alert-info" role="alert">
           <p>@ViewBag.admis candidats sont admis </p>
        </div>
    </center>
